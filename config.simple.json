{
  "qqConfig": {
    "botId": # 机器人账号 #,
    "password": # 机器人登录密码 16位MD5 #,
    "version": # 登录协议 iPad MacOS QiDian AndroidPhone AndroidWatch #,
    "auth": # 无token时的认证方式 qr(二维码) pwd(账号+密码) #
  },
  "discordConfig": {
    "botId": # 机器人的id(u64) #,
    "botToken": # 机器人的token #
  },
  "telegramConfig": {
    "apiId": # apiId(i32) # ,
    "apiHash": # apiHash # ,
    "botToken": # botToken #
  },
  "bridges": [
    {
      "discord": {
        "id": # 频道的Webhook id(u64) #,
        "token": # 频道的token #,
        "channelId": # 频道的id(u64) #
      },
      "tgGroup": # telegram群号 #,
      "qqGroup": # qq群号(u64) #,
      "enable": true
    }
  ]
}